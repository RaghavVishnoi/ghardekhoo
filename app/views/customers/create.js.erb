<% if @operation == 'retailer_details' %>
	$('#userDetailsForm').attr('style', 'display: none;')
	$('#retailerDetailsForm').html('<%=j render partial: "commons/retailer_details", locals: {retailer: @retailer}%>')
<% elsif @show_errors != true %>
	window.location = '/'
<% else %>
	renderFlash('div.pop_flash_container', $("<%=j render 'layouts/flash_message'%>"));
	scrollTop()
	<% flash.discard %>
<% end %>
stop_spin();
discard_flash();