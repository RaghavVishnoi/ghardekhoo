<div id="category-list-contaiiner">
	<%= render 'filter_list', states: @states, cities: @cities, default_state: @default_state, default_city: @default_city, default_category: @default_category %>
</div>

<div class="slider-add-container">
  <%= render 'commons/template/slide_small', urls: web_slides('list') %>
</div>

<div class="main-container">
  <div class="container">
    <div class="row">
      <div id="list-filter">
      	<%= render 'filters' %> 
      </div>

      <div id="retailer-list">
      	<%= render 'list', retailer_products: [] %> 
      </div>	
    </div>
  </div>
</div>
<%= render 'commons/hidden_forms' %>
<%= render 'commons/search_retailers_form'%>

<script>
  $(document).ready(function(){
    start_spinner('customer-list');
   	search_retailers()
  })
</script>