<% if @show_errors != true %>
	<% if @retailer.employee.present? %>
		window.location = '/employees/retailers'
	<% else %>
		window.location = '<%= @redirect_url %>'
	<% end %>	
<% else %>
	renderFlash('div.flash_container', $("<%=j render 'layouts/flash_message'%>"));
	scrollTop()
	<% flash.discard %>
	stop_spin();
	discard_flash();
<% end %>